includes:
    - vendor/larastan/larastan/extension.neon
parameters:
    # La configurazione del livello di analisi di PHPStan
    level: 5
    paths:
        - app
        - database
        - routes
    excludePaths:
        - tests
    ignoreErrors:
        - '#Relation .* is not found in App\\Models\\Booking model#'
